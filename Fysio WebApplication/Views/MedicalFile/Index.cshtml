@model IEnumerable<Fysio_WebApplication.Models.MedicalFile>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiagnosisCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntakeTherapistId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntakeSupervision)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfCreation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfDischarge)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiagnosisCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntakeTherapistId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntakeSupervision)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfCreation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfDischarge)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id}) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) | 
                @Html.ActionLink("Notes", "Notes", new {id = item.Id})
            </td>
        </tr>
}
    </tbody>
</table>
